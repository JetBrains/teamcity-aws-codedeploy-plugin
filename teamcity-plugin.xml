<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>aws-codedeploy-plugin</name>
        <display-name>AWS CodeDeploy</display-name>
        <version>@Plugin_Version@</version>
        <description>Build runner for deploying application to AWS EC2 and on-premises instances using AWS CodeDeploy</description>
        <download-url>https://github.com/JetBrains/teamcity-aws-codedeploy-plugin</download-url>
        <vendor>
            <name>JetBrains, s.r.o.</name>
            <url>https://www.jetbrains.com</url>
        </vendor>
    </info>
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>